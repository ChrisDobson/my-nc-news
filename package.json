{
  "name": "be-nc-news",
  "version": "1.0.0",
  "description": "A backend API for NC-News, built with Node.js and PostgreSQL.",
  "main": "listen.js",
  "scripts": {
    "start": "node listen.js",
    "seed-prod": "NODE_ENV=production npm run seed",
